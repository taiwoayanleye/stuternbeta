<h1>Listing saved_student_profiles</h1>

<table>
  <thead>
    <tr>
      <th>School text</th>
      <th>Year text</th>
      <th>Skill text</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @saved_student_profiles.each do |saved_student_profile| %>
      <tr>
        <td><%= saved_student_profile.school_text %></td>
        <td><%= saved_student_profile.year_text %></td>
        <td><%= saved_student_profile.skill_text %></td>
        <td><%= link_to 'Show', saved_student_profile %></td>
        <td><%= link_to 'Edit', edit_saved_student_profile_path(saved_student_profile) %></td>
        <td><%= link_to 'Destroy', saved_student_profile, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Saved student profile', new_saved_student_profile_path %>
